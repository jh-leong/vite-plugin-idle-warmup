{
  "name": "vite-plugin-idle-warmup",
  "version": "0.0.0",
  "description": "ğŸ“¦ Vite plugin for idle warming up the transform cache",
  "keywords": [
    "vite",
    "vite-plugin",
    "vite-plugin-warmup",
    "idle-warmup"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jh-leong/vite-plugin-idle-warmup.git"
  },
  "bugs": "https://github.com/jh-leong/vite-plugin-idle-warmup/issues",
  "packageManager": "pnpm@8.14.0",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "vite": ">=4.3.1"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "fast-glob": "^3.2.12",
    "vite-plugin-warmup": "^0.1.0"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.12",
    "@types/debug": "^4.1.12",
    "@types/node": "^20.10.7",
    "@vue/eslint-config-standard": "~8.0.1",
    "@vue/eslint-config-typescript": "~12.0.0",
    "bumpp": "^9.2.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "~8.3",
    "eslint-plugin-vue": "~9",
    "lint-staged": "^15.2.0",
    "pnpm": "^8.14.0",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.9.0",
    "tsx": "^4.0.0",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vite": "^5.0.11",
    "vitest": "^1.1.3"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --fix"
  }
}
